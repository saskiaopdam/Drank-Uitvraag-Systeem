{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "format": "int64"
    },
    "username": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "preference": {
      "type": "object",
      "$ref": "preference.json"
    }
  },
  "required": [
    "username", "firstName", "lastName"
  ]
}
